<div class="col-md-7 search-container">
  <div class="row search-details">
    <div class="row dates">
      Dates
      <input type="text"
             name="checkin"
             id="checkin"
             placeholder="Check In"
             value="<%= PetBnB.router._checkin %>">
      <input type="text"
             name="checkout"
             id="checkout"
             placeholder="Check Out"
             value="<%= PetBnB.router._checkout %>">
    </div>
    <div class="row price-range">
      Price Range
    </div>
  </div>
  <div class="row search-results">
    Search Results
    <ul class="list-group">
      <% listings.each(function (listing) { %>
        <li class="list-group-item pull-left">
          <a href="#/listings/<%= listing.id %>">
            <% if (listing.images().length) { %>
              <img src="<%= listing.images().first().url %>" alt="" />
            <% } %>
          </a>
          <a href="#/listings/<%= listing.id %>">
            <%= listing.escape('name') %>
          </a>
        </li>
      <% }) %>
    </ul>
  </div>
</div>
<div class="col-md-5 search-map">
  Map
</div>
