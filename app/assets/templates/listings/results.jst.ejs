<div class="col-md-5 search-map">
  Map
</div>

<div class="col-md-7 search-container">
  <div class="row search-details">
    <div class="dates panel-body">
      <div class="row">
        <div class="col-md-2 col-sm-12">
          <label>Dates</label>
        </div>
        <div class="col-md-10 col-sm-12">
          <div class="row row-condensed">
            <div class="col-md-4 col-sm-6">
              <input type="text"
                     name="checkin"
                     id="checkin"
                     placeholder="Check In"
                     value="<%= PetBnB.checkin %>">
            </div>
            <div class="col-md-4 col-sm-6">
              <input type="text"
                     name="checkout"
                     id="checkout"
                     placeholder="Check Out"
                     value="<%= PetBnB.checkout %>">
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="price-range panel-body">
      <div class="row">
        <div class="col-md-2 col-sm-12">
          <label>Price Range</label>
        </div>
        <div class="col-md-10 col-sm-12">
          <div id="slider">
          </div>
          <div class="row">
            <span class="pull-left" id="slider-min"></span>
            <span class="pull-right" id="slider-max"></span>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
